<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sjtu.ExcelApp">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo3"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">

        <activity android:name=".Activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.DepartmentActivity" />
        <activity android:name=".Activity.LoginActivity" />
        <activity android:name=".Activity.UserActivity" />
        <activity android:name=".Activity.ModifyEmailActivity" />
        <activity android:name=".Activity.ModifyPasswordActivity" />
        <activity android:name=".Activity.ModifyPhoneActivity" />
        <activity android:name=".Activity.WebViewActivity" />
    </application>

</manifest>